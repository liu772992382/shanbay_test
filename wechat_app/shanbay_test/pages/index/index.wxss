.container {
  background-color: #fff;
  padding: 5%;
} 

.data{
  margin: 0 5%;
  display: flex;
  align-items: center;
  flex-direction: column;
  padding: 20% 0;
  border-bottom:1px solid #eee ;
}
.data .dataNum{
  color: #209E85;
  font: bold 48px/52px arial;
}
.flex-wrp {
  display: flex;
  align-items: center;
  padding: 15% 0 20% 0;
}

.flex-wrp .flex-item {
  width: 50%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.flex-wrp .flex-item .itemName,.data .itemName{
  font-size: 12px;
  color:#999;
}

.flex-wrp .flex-item .itemNum{
  color: #666;
  font:bold 25px/35px arial;
}
.startBtn{
  font-size: 14px;
  background-color: #209E85;
  width: 30%;
  color: #fff;
  border-radius: 20px;
}